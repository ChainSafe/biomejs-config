{
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
  "packages": {
    ".": {
      "changelog-path": "CHANGELOG.md",
      "changelog-type": "github",
      "release-type": "node",
      "bump-minor-pre-major": false,
      "bump-patch-for-minor-pre-major": false,
      "draft": false,
      "prerelease": false,
      "changelog-sections": [
        {
          "type": "feat",
          "section": "Features",
          "hidden": false
        },
        {
          "type": "refactor",
          "section": "Improvements",
          "hidden": false
        },
        {
          "type": "fix",
          "section": "Bug Fixes",
          "hidden": false
        },
        {
          "type": "chore",
          "section": "Maintenance",
          "hidden": false
        },
        {
          "type": "ci",
          "section": "Maintenance",
          "hidden": false
        },
        {
          "type": "deps",
          "section": "Maintenance",
          "hidden": false
        },
        {
          "type": "style",
          "section": "Maintenance",
          "hidden": false
        }
      ]
    }
  }
}
